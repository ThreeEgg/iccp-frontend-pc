{
  "name": "iccp-fe-pc",
  "version": "1.0.0",
  "private": true,
  "description": "iccp frontend for pc platform",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env  node server/index.js",
    "build": "cross-env GIT_VERSION=`git rev-list HEAD --count` next build",
    "start": "cross-env NODE_ENV=production GIT_VERSION=`git rev-list HEAD --count` node server/index.js",
    "export": "npm run build && next export",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "pm2": "pm2 start pm2.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": null,
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": [
      "stylelint --fix \"src/**/*.less\" --syntax less",
      "git add"
    ],
    "**/*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "antd": "^4.0.1",
    "array-move": "^2.2.1",
    "axios": "^0.19.2",
    "chat-react": "^0.1.2",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "dva-immer": "^0.4.19",
    "dva-loading": "^3.0.20",
    "dva-no-router": "^1.2.1",
    "echarts": "^4.7.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "rc-tween-one": "^2.6.8",
    "react": "^16.13.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.0",
    "react-id-swiper": "^3.0.0",
    "react-intl-universal": "^2.2.2",
    "react-ratings-declarative": "^3.4.1",
    "react-redux": "^7.2.0",
    "react-sortable-hoc": "^1.11.0",
    "shortid": "^2.2.15",
    "swiper": "^5.3.6",
    "umi-request": "^1.2.19"
  },
  "devDependencies": {
    "@umijs/fabric": "^2.0.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "babel-plugin-import": "^1.13.0",
    "css-loader": "^3.4.2",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-vars-to-js": "^1.3.0",
    "lint-staged": "^10.0.8",
    "next": "^9.2.2",
    "next-compose-plugins": "^2.2.0",
    "stylelint": "^13.2.0"
  }
}
